    {{--Cuộn ngang bằng button--}}{{--
    <script>
        $('#right-button').click(function() {
            event.preventDefault();
            $('#content').animate({
                scrollLeft: "+=203px"
            }, "10");
        });

        $('#left-button').click(function() {
            event.preventDefault();
            $('#content').animate({
                scrollLeft: "-=200px"
            }, "10");
        });
        $('#date-icon').click(function () {
           $('#date').css("display","");
           $(this).css("display","none");
        });
        $(window).width().change(function () {
            if ($(window).width() < 1660) {
                event.preventDefault();
                $('#content').animate({
                    scrollLeft: "-=200px"
                }, "10");
            };
        });

    </script>--}}
