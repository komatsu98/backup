        $('#div-1').click(function(){
            var dateControl = document.querySelector('input[type="date"]');

            dateControl.stepDown(3);
            var daySelected = 'a' + dateControl.value.replace(/-/g,"") ;
            var daySelectedContent = 'b' + dateControl.value.replace(/-/g,"") ;
            document.getElementById('day-div-4').innerHTML = dateControl.value.slice(8,10);
            document.getElementById('month-div-4').innerHTML = monthOfYear (dateControl.value);
            // document.getElementById('in-div-4').innerHTML = eval(daySelected).in;
            // document.getElementById('out-div-4').innerHTML = eval(daySelected).out;
            document.getElementById('dw-div-4').innerHTML = getDayOfWeek (dateControl.value);
            // document.getElementById('demo2').innerHTML = document.getElementById(daySelectedContent).innerHTML;


