                document.getElementById('day-div-1').innerHTML = dateControl.value.slice(8,10);
                document.getElementById('month-div-1').innerHTML = monthOfYear (dateControl.value);
                document.getElementById('dw-div-1').innerHTML = getDayOfWeek (dateControl.value);
             /*   document.getElementById('in-div-1').innerHTML = eval(daySelected).in;
                document.getElementById('out-div-1').innerHTML = eval(daySelected).out;*/
                dateControl.stepUp(3);

                dateControl.stepDown(2);
                var daySelected = 'a' + dateControl.value.replace(/-/g,"") ;
                document.getElementById('day-div-2').innerHTML = dateControl.value.slice(8,10);
                document.getElementById('month-div-2').innerHTML = monthOfYear (dateControl.value);
                document.getElementById('dw-div-2').innerHTML = getDayOfWeek (dateControl.value);
           /*     document.getElementById('in-div-2').innerHTML = eval(daySelected).in;
                document.getElementById('out-div-2').innerHTML = eval(daySelected).out;*/
                dateControl.stepUp(2);

                dateControl.stepDown(1);
                var daySelected = 'a' + dateControl.value.replace(/-/g,"") ;
                document.getElementById('day-div-3').innerHTML = dateControl.value.slice(8,10);
                document.getElementById('month-div-3').innerHTML = monthOfYear (dateControl.value);
                document.getElementById('dw-div-3').innerHTML = getDayOfWeek (dateControl.value);
   /*             document.getElementById('in-div-3').innerHTML = eval(daySelected).in;
                document.getElementById('out-div-3').innerHTML = eval(daySelected).out;*/
                dateControl.stepUp(1);


                dateControl.stepUp(1);
                var daySelected = 'a' + dateControl.value.replace(/-/g,"") ;
                document.getElementById('day-div-5').innerHTML = dateControl.value.slice(8,10);
                document.getElementById('month-div-5').innerHTML = monthOfYear (dateControl.value);
                document.getElementById('dw-div-5').innerHTML = getDayOfWeek (dateControl.value);
             /*   document.getElementById('in-div-5').innerHTML = eval(daySelected).in;
                document.getElementById('out-div-5').innerHTML = eval(daySelected).out;*/
                dateControl.stepDown(1);

