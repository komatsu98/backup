    {{--convert ngay` sang tieng Viet--}}
    <script>
        function getDayOfWeek(date) {
            var dayOfWeek = new Date(date).getDay();
            return isNaN(dayOfWeek) ? null : ['Chủ Nhật', 'Thứ Hai', 'Thứ Ba', 'Thứ Tư', 'Thứ Năm', 'Thứ Sáu', 'Thứ Bảy'][dayOfWeek];
        };
        function monthOfYear(date) {
            var monthOfYear = new Date(date).getMonth();
            return isNaN(monthOfYear) ? null : ['Tháng 1', 'Tháng 2', 'Tháng 3', 'Tháng 4', 'Tháng 5', 'Tháng 6', 'Tháng 7', 'Tháng 8', 'Tháng 9', 'Tháng 10', 'Tháng 11', 'Tháng 12'][monthOfYear];
        }
    </script>

    <script>
        $('#date-icon').click(function () {
            $('#date').css("display","");
            $(this).css("display","none");
        });
        $(document).ready(function(){
            document.getElementById('date').valueAsDate = new Date();
            var dateControl = document.querySelector('#date');
            var daySelected = 'a' + dateControl.value.replace(/-/g,"") ;
            var daySelectedContent = 'b' + dateControl.value.replace(/-/g,"") ;
            document.getElementById('day-div-4').innerHTML = dateControl.value.slice(8,10);
            document.getElementById('month-div-4').innerHTML = monthOfYear (dateControl.value)
            // document.getElementById('in-div-4').innerHTML = eval(daySelected).in;
            // document.getElementById('out-div-4').innerHTML = eval(daySelected).out;
            document.getElementById('dw-div-4').innerHTML = getDayOfWeek (dateControl.value)
            // document.getElementById('demo2').innerHTML = document.getElementById(daySelectedContent).innerHTML;




            dateControl.stepDown(3);
            var daySelected = 'a' + dateControl.value.replace(/-/g,"") ;
            document.getElementById('day-div-1').innerHTML = dateControl.value.slice(8,10);
            document.getElementById('month-div-1').innerHTML = monthOfYear (dateControl.value)
            // document.getElementById('in-div-1').innerHTML = eval(daySelected).in;
            // document.getElementById('out-div-1').innerHTML = eval(daySelected).out;
            document.getElementById('dw-div-1').innerHTML = getDayOfWeek (dateControl.value)
            dateControl.stepUp(3);

            dateControl.stepDown(2);
            var daySelected = 'a' + dateControl.value.replace(/-/g,"") ;
            document.getElementById('day-div-2').innerHTML = dateControl.value.slice(8,10);
            document.getElementById('month-div-2').innerHTML = monthOfYear (dateControl.value)
            // document.getElementById('in-div-2').innerHTML = eval(daySelected).in;
            // document.getElementById('out-div-2').innerHTML = eval(daySelected).out;
            document.getElementById('dw-div-2').innerHTML = getDayOfWeek (dateControl.value)
            dateControl.stepUp(2);

            dateControl.stepDown(1);
            var daySelected = 'a' + dateControl.value.replace(/-/g,"") ;
            document.getElementById('day-div-3').innerHTML = dateControl.value.slice(8,10);
            document.getElementById('month-div-3').innerHTML = monthOfYear (dateControl.value)
            // document.getElementById('in-div-3').innerHTML = eval(daySelected).in;
            // document.getElementById('out-div-3').innerHTML = eval(daySelected).out;
            document.getElementById('dw-div-3').innerHTML = getDayOfWeek (dateControl.value)
            dateControl.stepUp(1);


            dateControl.stepUp(1);
            var daySelected = 'a' + dateControl.value.replace(/-/g,"") ;
            document.getElementById('day-div-5').innerHTML = dateControl.value.slice(8,10);
            document.getElementById('month-div-5').innerHTML = monthOfYear (dateControl.value)
            // document.getElementById('in-div-5').innerHTML = eval(daySelected).in;
            // document.getElementById('out-div-5').innerHTML = eval(daySelected).out;
            document.getElementById('dw-div-5').innerHTML = getDayOfWeek (dateControl.value)
            dateControl.stepDown(1);

            dateControl.stepUp(2);
            var daySelected = 'a' + dateControl.value.replace(/-/g,"") ;
            document.getElementById('day-div-6').innerHTML = dateControl.value.slice(8,10);
            document.getElementById('month-div-6').innerHTML = monthOfYear (dateControl.value)
            // document.getElementById('in-div-6').innerHTML = eval(daySelected).in;
            // document.getElementById('out-div-6').innerHTML = eval(daySelected).out;
            document.getElementById('dw-div-6').innerHTML = getDayOfWeek (dateControl.value)
            dateControl.stepDown(2);

            dateControl.stepUp(3);
            var daySelected = 'a' + dateControl.value.replace(/-/g,"") ;
            document.getElementById('day-div-7').innerHTML = dateControl.value.slice(8,10);
            document.getElementById('month-div-7').innerHTML = monthOfYear (dateControl.value)
            // document.getElementById('in-div-7').innerHTML = eval(daySelected).in;
            // document.getElementById('out-div-7').innerHTML = eval(daySelected).out;
            document.getElementById('dw-div-7').innerHTML = getDayOfWeek (dateControl.value)
            dateControl.stepDown(3);
        });

